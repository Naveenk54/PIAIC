//=========================== REVISION OF LAST CLASS ================================
let FirstName = "Naveen";
let LastName = "Kumar";
let SecondPerson = "Asad";

if(FirstName === "Naveen" && LastName === "Kumar" || SecondPerson === "Asad"){
    console.log("You're not allowed");
}
else{
    console.log("You're allowed");
}
