function Greet(name: string){
    console.log("Hello", name);
}

Greet("Naveen");
//----------------------------------------------------------------
function sum(num1:number , num2:number){
    console.log(num1 + num2);
}
sum( 5 , 10 );
//----------------------------------------------------------------
function Sum(num1: number , num2: number){
    // console.log(num1 + num2);
    return num1 + num2;
}
let ans = Sum( 5 , 10 );

function multiply(num1: number){
    console.log(num1 * 2);
}
multiply(ans)
